		<!-- custom javascript -->
		<scripts>
			<![CDATA[
                $(document).ready(function() {
                    
                });
            ]]>
		</scripts>

		<!-- custom styles -->
		<styles>
			<![CDATA[
                
            ]]>
		</styles>
	</configuration>
</extentreports>